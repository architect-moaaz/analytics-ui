{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "no-unused-vars": 1,
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "react/jsx-uses-vars": 1
  },
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },
  "parser": "react-scripts/node_modules/babel-eslint",
  "globals": {
    "window": true,
    "document": true,
    "console": true,
    "event": true,
    "Promise": true,
    "setTimeout": true,
    "__API__": true,
    "process": true,
    "require": true
  }
}
